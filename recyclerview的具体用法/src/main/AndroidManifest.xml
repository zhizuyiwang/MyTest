<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hsf.myRecyclerView">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.LinkageListViewActivity">
        </activity>
        <activity android:name=".activity.FloatingButtonActivity">
        </activity>
        <activity android:name=".activity.ToolBarHintActivity">
        </activity>
        <activity android:name=".activity.SimpleRecyclerViewActivity">
        </activity>
        <activity android:name=".activity.CoordinatorLayout">
        </activity>
        <activity android:name=".activity.LayoutManagerActivity">
        </activity>
        <activity android:name=".activity.FloatingActionButtonActivity">
        </activity>
    </application>

</manifest>